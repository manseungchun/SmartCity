<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout3"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".Fragment3">

    <Button
        android:id="@+id/UpdateBtn"
        android:layout_width="149dp"
        android:layout_height="59dp"
        android:layout_marginBottom="44dp"
        android:backgroundTint="#2DB400"
        android:fontFamily="@font/jua"
        android:text="정보수정"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/UpdateCall"
        android:layout_width="220dp"
        android:layout_height="37dp"
        android:layout_marginBottom="50dp"
        android:ems="10"
        android:fontFamily="@font/jua"
        android:hint="변경하실 새 휴대폰번호를 입력해주세요"
        android:inputType="textPersonName"
        android:textSize="13sp"
        app:layout_constraintBottom_toTopOf="@+id/UpdateBtn"
        app:layout_constraintStart_toStartOf="@+id/UpdateAddr"
        app:layout_constraintTop_toBottomOf="@+id/UpdateAddr" />

    <TextView
        android:id="@+id/textView37"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/jua"
        android:text="휴대폰번호"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/UpdateCall"
        app:layout_constraintStart_toStartOf="@+id/textView36"
        app:layout_constraintTop_toTopOf="@+id/UpdateCall" />

    <TextView
        android:id="@+id/textView36"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/jua"
        android:text="주소"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/UpdateAddr"
        app:layout_constraintStart_toStartOf="@+id/textView18"
        app:layout_constraintTop_toTopOf="@+id/UpdateAddr" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/jua"
        android:text="이름"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/UpdateName"
        app:layout_constraintStart_toStartOf="@+id/textView16"
        app:layout_constraintTop_toTopOf="@+id/UpdateName" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/jua"
        android:text="비밀번호"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/UpdatePw"
        app:layout_constraintStart_toStartOf="@+id/textView9"
        app:layout_constraintTop_toTopOf="@+id/UpdatePw" />

    <Button
        android:id="@+id/button"
        android:layout_width="350dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:backgroundTint="#2DB400"
        android:fontFamily="@font/jua"
        android:text="회원정보수정"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        app:cornerRadius="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvHome" />

    <TextView
        android:id="@+id/tvHome"
        android:layout_width="0dp"
        android:layout_height="50dp"
        android:background="#2DB400"
        android:fontFamily="@font/jua"
        android:gravity="center"
        android:text="  국민픽서"
        android:textAlignment="viewStart"
        android:textColor="#FFFFFF"
        android:textSize="28sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/loginbtn"
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="15dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/login2" />

    <ImageView
        android:id="@+id/logoutbtn"
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_marginTop="4dp"
        android:src="@drawable/logout"
        app:layout_constraintEnd_toEndOf="@+id/loginbtn"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/textView"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/lemon"
        android:textColor="#FFFFFF"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/loginbtn"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:fontFamily="@font/jua"
        android:text="아이디"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/UpdateId"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/UpdateId" />

    <TextView
        android:id="@+id/UpdateId"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="60dp"
        android:layout_marginTop="50dp"
        android:layout_marginBottom="20dp"
        android:fontFamily="@font/jua"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/UpdatePw"
        app:layout_constraintStart_toEndOf="@+id/textView9"
        app:layout_constraintTop_toBottomOf="@+id/button" />

    <EditText
        android:id="@+id/UpdatePw"
        android:layout_width="220dp"
        android:layout_height="37dp"
        android:layout_marginBottom="20dp"
        android:ems="10"
        android:fontFamily="@font/jua"
        android:hint="변경하실 새 비밀번호를 입력해주세요"
        android:inputType="textPersonName"
        android:textSize="13sp"
        app:layout_constraintBottom_toTopOf="@+id/UpdateName"
        app:layout_constraintStart_toStartOf="@+id/UpdateId"
        app:layout_constraintTop_toBottomOf="@+id/UpdateId" />

    <TextView
        android:id="@+id/UpdateName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="20dp"
        android:fontFamily="@font/jua"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/UpdateAddr"
        app:layout_constraintStart_toStartOf="@+id/UpdatePw"
        app:layout_constraintTop_toBottomOf="@+id/UpdatePw" />

    <EditText
        android:id="@+id/UpdateAddr"
        android:layout_width="220dp"
        android:layout_height="37dp"
        android:layout_marginBottom="20dp"
        android:ems="10"
        android:fontFamily="@font/jua"
        android:hint="변경하실 새 주소를 입력해주세요"
        android:inputType="textPersonName"
        android:textSize="13sp"
        app:layout_constraintBottom_toTopOf="@+id/UpdateCall"
        app:layout_constraintStart_toStartOf="@+id/UpdateName"
        app:layout_constraintTop_toBottomOf="@+id/UpdateName" />


</androidx.constraintlayout.widget.ConstraintLayout>